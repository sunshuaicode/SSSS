package com.gxun.task.entity;

public class Teacher {
   private String t_no;
   private String t_name;
   private String t_password;
   private String t_phonenum;
   private String t_college;
public String getT_no() {
	return t_no;
}
public void setT_no(String t_no) {
	this.t_no = t_no;
}
public String getT_name() {
	return t_name;
}
public void setT_name(String t_name) {
	this.t_name = t_name;
}
public String getT_password() {
	return t_password;
}
public void setT_password(String t_password) {
	this.t_password = t_password;
}
public String getT_phonenum() {
	return t_phonenum;
}
public void setT_phonenum(String t_phonenum) {
	this.t_phonenum = t_phonenum;
}
public String getT_college() {
	return t_college;
}
public void setT_college(String t_college) {
	this.t_college = t_college;
}
   
   


}
